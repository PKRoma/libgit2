#!/bin/bash -e
#
# Description: blame a file in the linux repository

. "$(dirname "$0")/benchmark_helpers.sh"

needs_repo linux

gitbench --prepare "clone_repo linux && cd linux && git reset --hard v6.9" \
         --warmup 5 \
         --chdir "linux" \
         -- \
         --no-pager blame "Makefile"
